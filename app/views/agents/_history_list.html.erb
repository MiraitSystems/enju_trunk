<div id="history">
  <ul>
    <%- @agent.versions.reverse.each_with_index do |version, i| -%>
      <li>
        <%- unless @agent.user.blank? -%>
          <%# = link_to l(version.created_at), user_path(@agent.user, :version_id => version.id) -%>
          <%= l(version.created_at) -%>
        <%- else -%>
          <%#= link_to l(version.created_at), agent_path(@agent, :version_id => version.id) -%>
          <%= l(version.created_at) -%>
        <%- end -%>
        <%- if i == 0 -%>
          (<%= t('page.current_version') -%>)
        <%- end -%>
      </li>
    <%- end -%>
  </ul>
</div>
