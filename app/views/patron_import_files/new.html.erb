<div id="content_detail" class="ui-corner-all">
  <h1 class="title"><%= t('page.new', :model => t('activerecord.models.patron_import_file')) -%></h1>
<div id="content_list">

<%= form_for(@patron_import_file, :html => { :multipart => true }) do |f| -%>
  <%= f.error_messages -%>

  <div class="field">
    <%= f.label t('page.file') -%><br />
    <%= f.file_field :patron_import -%>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<%- end -%>

</div>
</div>

<div id="submenu" class="ui-corner-all">
  <ul>
    <li><%= link_to t('page.back'), patron_import_files_path -%></li>
  </ul>
</div>
