<% unless @family_users.blank? %>
<div id="family_users">
  <div><strong><%= t('family.already_family_with') -%></strong></div> 
  <%= check_box_tag 'out_of_family', "1" -%><%= t('family.out_of_family') -%>
  <ul>
    <% @family_users.each do |f_user| %>
      <li><%= f_user.patron.full_name -%>(<%= f_user.username -%>)</li>
    <% end %>
  </ul>
</div>
<hr>
<%= t('family.will_change_family') -%>
<% end %>

