<div id="related_manifestation">
  <ul>
    <%- manifestation.original_manifestations.each do |manifestation| -%>
        <li>
          [M]
          <%= link_to manifestation.original_title, manifestation -%>
          <%= form_icon(manifestation.carrier_type) -%>
          <%= link_to image_tag('icons/world_go.png', :size => '16x16', :alt => ('Web'), :title => t('tooltip.web')), manifestation.access_address if manifestation.access_address? -%>
          <%= agents_list(manifestation.creates.order(:position).readable_by(current_user)) -%>
          <%= agents_list(manifestation.realizes.order(:position).readable_by(current_user)) -%>
          <%= agents_list(manifestation.produces.order(:position).readable_by(current_user)) -%>
          <%- if manifestation.pub_date? -%>
            (<%= manifestation.pub_date -%>)
          <%- end -%>
        </li>
    <%- end -%>
    <%- manifestation.derived_manifestations.each do |manifestation| -%>
        <li>
          [M]
          <%= link_to manifestation.original_title, manifestation -%>
          <%= form_icon(manifestation.carrier_type) -%>
          <%= link_to image_tag('icons/world_go.png', :size => '16x16', :alt => ('Web'), :title => t('tooltip.web')), manifestation.access_address if manifestation.access_address? -%>
          <%= agents_list(manifestation.creates.order(:position).readable_by(current_user)) -%>
          <%= agents_list(manifestation.realizes.order(:position).readable_by(current_user)) -%>
          <%= agents_list(manifestation.produces.order(:position).readable_by(current_user)) -%>
          <%- if manifestation.pub_date? -%>
            (<%= manifestation.pub_date -%>)
          <%- end -%>
        </li>
    <%- end -%>
  </ul>
</div>
