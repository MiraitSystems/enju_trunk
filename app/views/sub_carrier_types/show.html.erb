<div id="content_detail" class="ui-corner-all">
  <h1 class="title"><%= t('page.showing', :model => t('activerecord.models.sub_carrier_type')) %></h1>
  <div id="content_list">

    <p id="notice"><%= notice %></p>

    <p>
      <b><%= t('activerecord.attributes.sub_carrier_type.name') %>:</b>
      <%= @sub_carrier_type.name %>
    </p>

    <p>
      <b><%= t('activerecord.attributes.sub_carrier_type.display_name') %>:</b>
      <%= @sub_carrier_type.display_name %>
    </p>

    <p>
      <b><%= t('activerecord.attributes.sub_carrier_type.carrier_type_id') %>:</b>
      <%= @sub_carrier_type.carrier_type.display_name.localize if @sub_carrier_type.carrier_type %>
    </p>

    <p>
      <b><%= t('activerecord.attributes.sub_carrier_type.nacsis_identifier') %>:</b>
      <%= @sub_carrier_type.nacsis_identifier %>
    </p>

    <p>
      <b><%= t('activerecord.attributes.sub_carrier_type.note') %>:</b>
      <%= @sub_carrier_type.note %>
    </p>
  </div>
</div>

<div id="submenu" class="ui-corner-all">
  <ul>
    <% if can? :create, SubCarrierType %>
      <li><%= link_to t('page.edit'), edit_sub_carrier_type_path(@sub_carrier_type) %>
    <% end %>
    <li><%= link_to t('page.back'), sub_carrier_types_path %>
  </ul>
</div>
