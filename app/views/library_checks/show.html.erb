<div id="content_detail" class="ui-corner-all">
<h1 class="title"><%= t('page.showing', :model => t('activerecord.models.library_check')) -%></h1>
<div id="content_list">

<p>
  <strong><%= t('activerecord.attributes.library_check.id') -%> :</strong>
  <%= @library_check.id -%> 
</p>

<p>
  <strong><%= t('activerecord.attributes.library_check.opeym') -%> :</strong>
  <%= @library_check.opeym -%> 
</p>

<p>
  <strong><%= t('activerecord.attributes.library_check.shelf_def_file') -%> :</strong>
  <%= @library_check.shelf_def_file -%>
</p>

<p>
  <strong><%= t('activerecord.attributes.library_check.operated_at') -%> :</strong>
  <td><% if @library_check.operated_at.blank? %><%= t('activerecord.attributes.library_check.not_yet') -%>
      <% else %><%= l(@library_check.operated_at) -%><% end %></td>
</p>

<p>
  <strong><%= t('activerecord.attributes.library_check.status') -%> :</strong>
  <%= localized_state(@library_check.state) -%> 
</p>

<p>
  <strong><%= t('activerecord.attributes.library_check.error_msg') -%> :</strong>
  <%= h(@library_check.error_msg) -%> 
</p>

</div>
</div>

<div id="submenu" class="ui-corner-all">
  <ul>
    <li><%= link_to t('page.edit'), edit_library_check_path(@library_check) -%> </li>
    <li><%= link_to t('activerecord.attributes.library_check.shelf_data'), library_check_library_check_shelves_path(@library_check) -%></li> 
    <li><%= link_to t('activerecord.attributes.library_check.download'), library_check_path(@library_check, :format => :download, :file => :pdf) -%> </li>
    <li><%= link_to t('activerecord.attributes.library_check.book_data'), library_check_libcheck_data_files_path(@library_check) -%> </li>
    <li><%= link_to t('activerecord.attributes.library_check.do'), {:mode => 'exec'} -%> </li>
    <li><%= link_to t('activerecord.attributes.library_check.resource_list'), library_check_path(@library_check, :format => :download, :file => :resource_list)-%> </li>
    <li><%= link_to t('activerecord.attributes.library_check.notfound_list'), library_check_path(@library_check, :format => :download, :file => :notfound_list)-%> 
</li>
    <li><%= link_to t('page.destroy'), @library_check, :confirm => t('page.are_you_sure'), :method => :delete -%>
    <li><%= link_to t('page.back_to_index'), library_checks_path -%> </li>
  </ul>
</div>
