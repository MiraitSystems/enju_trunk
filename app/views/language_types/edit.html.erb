<div id="content_detail" class="ui-corner-all">
  <h1 class="title"><%= t('page.editing', :model => t('activerecord.models.language_type')) -%></h1>
  <div id="content_list">
    <%= render 'form' %>
  </div>
</div>

<div id="submenu" class="ui-corner-all">
  <ul>
    <%- if can? :show, @language_type -%>
      <li><%= link_to t('page.show'), language_type_path(@language_type) -%></li>
    <%- end -%>
    <li><%= link_to t('page.back'), :back -%></li>
  </ul>
</div>
