<%= form_for(@system_configuration) do |f| %>
  <%= f.error_messages %>

  <div class="field">
    <%= f.label :keyname %><br />
    <%= f.text_field :keyname -%>
  </div>

  <div class="field">
    <%= f.label :v %><br />
    <%= f.text_field :v -%>
  </div>

  <div class="field">
    <%= f.label :typename %><br />
    <%= f.select(:typename, SystemConfiguration.typenames) -%>
  </div>

  <div class="field">
    <%= f.label :description %><br />
    <%= f.text_field :description -%>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
